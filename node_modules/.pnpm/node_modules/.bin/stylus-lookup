#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/stylus-lookup@5.0.1/node_modules/stylus-lookup/bin/node_modules:/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/stylus-lookup@5.0.1/node_modules/stylus-lookup/node_modules:/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/stylus-lookup@5.0.1/node_modules:/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/stylus-lookup@5.0.1/node_modules/stylus-lookup/bin/node_modules:/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/stylus-lookup@5.0.1/node_modules/stylus-lookup/node_modules:/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/stylus-lookup@5.0.1/node_modules:/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../stylus-lookup/bin/cli.js" "$@"
else
  exec node  "$basedir/../stylus-lookup/bin/cli.js" "$@"
fi
