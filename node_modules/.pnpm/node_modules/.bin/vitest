#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/vitest@1.6.0_@types+node@20.14.2_jsdom@24.1.0_lightningcss@1.25.1_terser@5.31.0/node_modules/vitest/node_modules:/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/vitest@1.6.0_@types+node@20.14.2_jsdom@24.1.0_lightningcss@1.25.1_terser@5.31.0/node_modules:/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/vitest@1.6.0_@types+node@20.14.2_jsdom@24.1.0_lightningcss@1.25.1_terser@5.31.0/node_modules/vitest/node_modules:/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/vitest@1.6.0_@types+node@20.14.2_jsdom@24.1.0_lightningcss@1.25.1_terser@5.31.0/node_modules:/Users/ayush.shrivastava/Github/insights-keycloak/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
